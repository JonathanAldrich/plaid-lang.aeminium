package plaid.typechecker.tests.longTest;

method main() {
	// tests selection of method based on lack of widening
	java.lang.System.out.println(java.lang.Math.min(1, 2));
	
	// tests that both method and constructor works under widening
	java.lang.System.out.println(java.lang.Long.valueOf(1));
	java.lang.System.out.println(java.lang.Long.new(2));
}