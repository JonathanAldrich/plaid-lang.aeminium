package plaid.typechecker.AST;

state NonePermission case of Permission {
	override method isWritable() {
		false;
	}

	override method split(p2) {
		match(p2) {
			// none => none/none
			case NonePermission {
				(p2, this)
			}
			default {
				new None;
			}
		};
	}
	
	override method ==(p2) {
		match(p2) {
			case NonePermission {
				true;
			}
			default {
				false;
			}
		};
	}
	
	override method merge(p2) {
		p2;
	}
}