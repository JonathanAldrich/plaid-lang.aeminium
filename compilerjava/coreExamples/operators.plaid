package coreExamples.operators;

import java.lang.System;
 
method optest() {
 	var x = 5 * 2 + 5;
 	System.out.print("The next values should all read 15: ");
 	System.out.println(x);
 	x =  5 + 0 * 3 + 10;
 	System.out.println(x);
 	x = 5 / 5 + 15 - 1;
 	System.out.println(x);
// 	x = 16;
// 	x = x - 1;
// 	System.out.println(x);
 	var z = x > (10 + 7);
 	plaid.lang.if (z) {
 		System.out.println("error!");
 	};
 }